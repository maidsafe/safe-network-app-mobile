             x:Class="MobileSnapp.Views.CustomViews.NavbarView">
    <ContentView.Content>
        <StackLayout Orientation="Horizontal"
                     Padding="10,0,0,0"
                     Spacing="10">
            <Image  Source="safelogowhite"
